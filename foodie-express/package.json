{
  "name": "foodie-express",
  "version": "1.0.0",
  "description": "Server for Foodie",
  "main": "server.js",
  "scripts": {
    "start": "node -r dotenv/config server.js",
    "dev": "nodemon -r dotenv/config server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-client": "cd ../foodie-react && npm run build && cd ../foodie-express",
    "renew-public": "rmdir /s /q public && mkdir public",
    "copy-build": "xcopy /s \"../foodie-react/dist\" public",
    "build": "npm run build-client && npm run renew-public && npm run copy-build"
  },
  "author": "Javen Lai Le Yu",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "express": "^4.18.2",
    "googleapis": "^110.0.0",
    "jsonwebtoken": "^9.0.0",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.9.0"
  },
  "devDependencies": {
    "dotenv": "^16.0.3",
    "nodemon": "^2.0.20"
  }
}
